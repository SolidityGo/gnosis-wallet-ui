    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.7",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-spring": "^9.3.2",
    "react-use": "^17.3.1",
    "react-use-gesture": "^5.0.0",
    "styled-components": "^5.3.3",
    "tachyons": "^4.12.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "prepare": "husky install",
