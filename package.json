    "@react-spring/parallax": "^9.3.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.3.1",
    "@types/howler": "^2.2.4",
    "@types/numeral": "^2.0.2",
    "@types/styled-components": "^5.1.15",
    "@usedapp/core": "^0.6.3",
    "antd": "^4.17.2",
    "classnames": "^2.3.1",
    "ethers": "^5.5.2",
